<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>RegistrationForm_v10 by Colorlib</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">



    <!-- LINEARICONS -->
    
    <link href="~/RegistrationContent/css/style.css" rel="stylesheet" />



    <link href="~/RegistrationContent/fonts/linearicons/style.css" rel="stylesheet" />
   

    <!-- STYLE CSS -->
    
<link href="~/RegistrationContent/css/style.css" rel="stylesheet" />
</head>



<body>



    <div class="wrapper">
        <div class="inner">
           
            <img src="~/RegistrationContent/images/image-1.png" alt="" class="image-1" />

            <form action="">
                <h3>New Account?</h3>
                <div class="form-holder">
                    <span class="lnr lnr-user"></span>
                    <input type="text" class="form-control" placeholder="PsID">
                </div>
                <div class="form-holder">
                    <span class="lnr lnr-user"></span>
                    <input type="text" class="form-control" placeholder="Name">
                </div>
                <div class="form-holder">
                    <span class="lnr lnr-envelope"></span>
                    <input type="text" class="form-control" placeholder="Address">
                </div>
                <div class="form-holder">
                    <span class="lnr lnr-eye"></span>
                    <input type="text" class="form-control" placeholder="Age">
                </div>
                <div class="form-holder">
                    <span class="lnr lnr-flag"></span>
                    <input type="text" class="form-control" placeholder="Nationality">
                </div>
                <div class="form-holder">
                    <span class="lnr lnr-phone-handset"></span>
                    <input type="text" class="form-control" placeholder="Contact">
                </div>
                <div class="form-holder">
                    <span class="lnr lnr-lock"></span>
                    <input type="password" class="form-control" placeholder="Password">
                </div>



                <button>
                    <span>Register</span>
                </button>
            </form>

            @*<img src="images/image-2.png" alt="" class="image-2">*@
            <img src="~/RegistrationContent/images/image-2.png" alt="" class="image-2" />



        </div>



    </div>



    
    <script src="~/RegistrationContent/js/jquery-3.3.1.min.js"></script>
    <script src="~/RegistrationContent/js/main.js"></script>



</body>
</html>